# ===========================================
# Rideshare App Configuration
# ===========================================
# Copy this file to .env and update values
# IMPORTANT: Never commit .env to version control!
# ===========================================

# API Configuration
API_BASE_URL=http://localhost:3000/api/v1
API_SERVER_URL=http://localhost:3000
API_TIMEOUT=30000

# App Identification - Set to your rideshare app ID
APP_ID=58

# App Info
APP_NAME=Rideshare
APP_DISPLAY_NAME=Rideshare

# Bundle Identifiers
IOS_BUNDLE_ID=com.bluestoneapps.rideshare
ANDROID_PACKAGE=com.bluestoneapps.rideshare

# Feature Flags
ENABLE_DEBUG_LOGGING=false
ENABLE_ANALYTICS=false
